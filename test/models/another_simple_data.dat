index_set("A index") [ "Place A1" "Place A2" ]
index_set("B index") [ "Place B" ]

a : compartment("A") [ "A index" ]
b : compartment("B") [ "B index" ]
connection("A graph") [
	a[ "Place A1" ] -> 
	#a["Place A2"] 
	b[ "Place B" ]
]

par_group("System") {
	par_datetime("Start date")
	[ 1990-01-01 ]

	par_datetime("End date")
	[ 1995-05-01 ]
}

module("So simple", 1, 0, 0) {
	par_group("A Pars") [ "A index" ] {
		par_real("initial a q")
		[ 100000 50 ]
	}
	
	par_group("B Pars") {
		par_real("initial b q")
		[ 0 ]
	}
}

