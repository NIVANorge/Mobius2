data_set {
	
"""
NOTE: Breiangen is set to be 60 deep since that is the lowest threshold.
"""
	
	time_step([1, day])
	
	series_interval(2015-01-01, 2020-12-31)
	
	
	basin : index_set("Basin index") [ "Mossesundet" ]
	
	bound_basin : index_set("Boundary basin index") [ "Breiangen" ]
	
	all_basin : index_set("All basins") @union(basin, bound_basin) 
	
	layer : index_set("Layer index") @sub(all_basin) [!
		"Mossesundet"  : [ 110 ]
		"Breiangen"  : [ 60 ]
	]
	
	lake : index_set("Lake") [ "Våg+Langen" "Mjær" "Store" "Vanem" ]
	
	lu : index_set("Landscape units") [ "Arable" "Semi-natural" ]
	
	sc : index_set("Subcatchment") [ "Kråkstadelva" "Våg+Langen sc" "Mjær sc" "Kure" "Vaaler" "Store sc" "Vanem sc" "Mosselva" ]
	
	water_body : index_set("Water body") @union(sc, lake, basin) 
	
	fjord_conn : index_set("Fjord connection index") @sub(basin) 
	
	position_map(layer) [!
		0 : 1
		6 : 2
		10 : 10
		60 : 20
	]
	
	connection("Downstream") {
		
		e : compartment("Epilimnion", lake)
		r : compartment("River", sc)
		
		directed_graph [ 
			r[ "Våg+Langen sc" ] -> e[ "Våg+Langen" ] -> e[ "Mjær" ] -> r[ "Kure" ] -> r[ "Vaaler" ] -> e[ "Store" ] -> e[ "Vanem" ] -> r[ "Mosselva" ]
			r[ "Kråkstadelva" ] -> r[ "Kure" ]
			r[ "Mjær sc" ] -> e[ "Mjær" ]
			r[ "Store sc" ] -> e[ "Store" ]
			r[ "Vanem sc" ] -> e[ "Vanem" ]
		]
	}
	
	connection("Fjord horizontal") {
		
		r : compartment("River", sc)
		l : compartment("Fjord layer", basin)
		b : compartment("Boundary layer", bound_basin)
		
		directed_graph(fjord_conn) [ 
			r[ "Mosselva" ] -> l[ "Mossesundet" ] -> b[ "Breiangen" ]
		]
	}
	
	series("C:/Data/simplycnp_oslofjord/mobius2_files/vansjo/vansjo_mobius_one_meteo.xlsx")
	
	series("mossesundet_observed.xlsx")
	
	series("breiangen_of800_boundary.xlsx")
	
	#series("breiangen_of800_boundary_chem.xlsx")
	
	quick_select("Fjord vars") [!
		"Temperature" : [ "Layer temperature" ]
		"Salinity" : [ "Layer salinity" ]
		"O₂" : [ "O₂ saturation" ]
		"chl-a" : [ "Layer chl-a" ]
		"DOC" : [ "concentration(Layer DOC, Layer water)" ]
		"DIN" : [ "concentration(Layer DIN, Layer water)" ]
		"DIP" : [ "concentration(Layer DIP, Layer water)" ]
	]
	
	quick_select("Fjord calib") [!
		"Temperature" : [ "Layer temperature" "Obs fjord Temperature" ]
		"Salinity" : [ "Layer salinity" "Obs fjord Salinity" ]
		"PO4" : [ "concentration(Layer DIP, Layer water)" "Obs fjord PO4" ]
		"NO3" : [ "concentration(Layer DIN, Layer water)" "Obs fjord NO3" ]
		"DOC" : [ "concentration(Layer DOC, Layer water)" "Obs fjord DOC" ]
		"chl-a" : [ "Layer chl-a" "Obs fjord Chl-a" ]
	]
	
	quick_select("River vars") [!
		"Q" : [ "Reach flow flux" "Obs Q" ]
		"Q to fjord" : [ "Reach discharge to fjord" "Obs Q" ]
		"DIN" : [ "concentration(River water DIN, Reach water volume)" "Obs DIN" ]
		"DON" : [ "concentration(River DON, Reach water volume)" "Obs DON" ]
		"TN" : [ "River TN" "Obs TN" ]
		"TOC" : [ "River TOC concentration" "Obs TOC" ]
		"TP" : [ "River TP" "Obs TP" ]
		"TDP" : [ "River TDP" "Obs TDP" ]
		"DIP" : [ "concentration(River DIP, Reach water volume)" "Obs DIP" ]
		"PP" : [ "concentration(River PP, Reach water volume)" "Obs PP" ]
		"SS" : [ "concentration(Suspended sediments, Reach water volume)" "Obs SS" ]
	]
	
	quick_select("Lake vars") [!
		"Temperature" : [ "Epilimnion temperature" "Obs Temperature" ]
		"Ice ind" : [ "Ice indicator" "Observed ice indicator Vanem" ]
		"DIN" : [ "concentration(Epilimnion DIN, Epilimnion volume)" "Obs DIN" ]
		"DON" : [ "concentration(Epilimnion DON, Epilimnion volume)" "Obs DON" ]
		"TN" : [ "Epilimnion TN" "Obs TN" ]
		"TOC" : [ "Epilimnion TOC" "Obs TOC" ]
		"TP" : [ "Epilimnion TP" "Obs TP" ]
