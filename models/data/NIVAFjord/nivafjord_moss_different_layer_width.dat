		"DIP" : [ "concentration(Epilimnion DIP, Epilimnion volume)" "Obs DIP" "Obs TDP" ]
		"O₂" : [ "concentration(Epilimnion O₂, Epilimnion volume)" "Obs O₂" ]
		"SS" : [ "Epilimnion STS" "Obs SS" ]
		"chl-a" : [ "Chlorophyll-a" "Obs chl-a" ]
